{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "attributes": "readonly",
    "getAttrs": "readonly",
    "getSectionIDs": "readonly",
    "on": "readonly",
    "setAttrs": "readonly",
    "skills": "readonly",
    "views": "readonly"
  }
}